# AGESIC Data Lake PoC - Environment Variables Template
# Copy this file to .env and fill in your values
# DO NOT commit .env file to version control

# AWS Configuration
AWS_PROFILE=your-aws-profile
AWS_DEFAULT_REGION=us-east-1
CDK_DEFAULT_ACCOUNT=123456789012
CDK_DEFAULT_REGION=us-east-1

# Project Configuration
PROJECT_NAME=agesic-datalake-poc
ENVIRONMENT=poc

# Kinesis Configuration
KINESIS_STREAM_NAME=agesic-dl-poc-data-stream

# Notification Configuration
NOTIFICATION_EMAIL=your-email@example.com

# CDK Configuration
CDK_BOOTSTRAP_QUALIFIER=hnb659fds

# Development Configuration (optional)
DEBUG=false
VERBOSE_LOGGING=false

# Local Development Overrides (optional)
# LOCAL_CONFIG_FILE=config.local.json
# LOCAL_ASSETS_PATH=./local-assets
